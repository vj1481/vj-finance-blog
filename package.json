    "chart.js": "^2.9.3",
    "esbuild": "^0.12.22",
    "gray-matter": "^4.0.2",
    "image-size": "1.0.0",
    "mdx-bundler": "^5.1.2",
    "next": "11.0.1",
    "next-themes": "^0.0.14",
    "pnpapi": "0.0.0",
    "postcss": "^8.3.5",
    "preact": "^10.5.13",
    "react": "17.0.2",
    "react-chartjs-2": "^2.11.2",
    "react-dom": "17.0.2",
    "react-ga-gtm": "^2.5.3-beta",
